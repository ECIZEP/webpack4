
{
    "compilerOptions": {
      "baseUrl": "./",
      "paths": {
        "@actions/*": ["src/actions/*"],
        "@components/*": ["src/components/"],
        "@commonComp/*": ["src/components/common/"],
        "@common/*": ["src/common/*"],
        "@reducers/*": ["src/reducers/*"],
        "@images/*": ["src/images/*"],
        "@lib/*": ["src/lib/*"],
        "@assets/*": ["src/assets/*"],
        "@util/*": ["src/utility/*"],
        "config/*": ["src/config/*"],
        "sources/*": ["src/sources/*"],
        "stores/*": ["src/stores/*"],
        "styles/*": ["src/styles/*"]
      }
    },
    "exclude": ["node_modules", "dist"],
    "include": ["src"]
}  